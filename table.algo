def somme_elements_distincts(tableau1, tableau2):
VAR
tableau1, tableau2
    sum = 0 : INTEGER ;
BEGIN
    for element in tableau1:
        if element not in tableau2:
            somme += element
    for element in tableau2:
        if element not in tableau1:
            somme += element
    return somme

# Exemple d'utilisation
jeu1 = [3, 1, 7, 9]
jeu2 = [2, 4, 1, 9, 3]
resultat = somme_elements_distincts(jeu1, jeu2)
print(resultat) # affiche 13

